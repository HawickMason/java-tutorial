{
  "title": "jdk-source-code-note",
  "description": "JDK源码阅读笔记",
  "language": "zh-hans",
   "plugins": [
       "splitter",
       "-github",
       "prism",
       "-highlight",
       "page-footer-ex",
       "anchor-navigation-ex",
       "expandable-chapters",
       "advanced-emoji",
       "katex"
    ],
  "pluginsConfig": {
    "prism": {
       "css": [
         "prismjs/themes/prism-okaidia.css"
       ]
    },
    "anchor-navigation-ex":{
        "associatedWithSummary":true,
        "multipleH1":false,
        "printLog":true
    },
     "page-footer-ex": {
         "copyright": "[mrcode](http://blog.csdn.net/mr_zhuqiang)",
         "markdown": true,
         "update_label": "<i>updated</i>",
         "update_format": "YYYY-MM-DD HH:mm:ss"
    },
    "theme-default": {
       "showLevel": true
    }
  }
} 